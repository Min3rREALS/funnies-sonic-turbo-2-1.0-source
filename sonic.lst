000014BC 8801                       		or.b	d1,d4	; write pixel
000014BE 5343                       		subq.w	#1,d3	; has an entire 8-pixel row been written?
000014C0 6600                       		bne.s	NemPCD_WritePixel_Loop	; if not, loop
000014C2 4ED3                       		jmp	(a3)	; otherwise, write the row to its destination, by doing a dynamic jump to NemPCD_WriteRowToVDP, NemDec_WriteAndAdvance, NemPCD_WriteRowToVDP_XOR, or NemDec_WriteAndAdvance_XOR
