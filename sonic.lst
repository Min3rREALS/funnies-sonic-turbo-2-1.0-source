00001464 3A42                       		movea.w	d2,a5	; and store it in a5 because there aren't any spare data registers
00001466 7608                       		moveq	#8,d3	; 8 pixels in a pattern row
00001468 7400                       		moveq	#0,d2
0000146A 7800                       		moveq	#0,d4
0000146C 6100 0000                  		bsr.w	NemDec_BuildCodeTable
00001470 1A18                       		move.b	(a0)+,d5	; get first byte of compressed data
00001472 E145                       		asl.w	#8,d5	; shift up by a byte
00001474 1A18                       		move.b	(a0)+,d5	; get second byte of compressed data
00001476 3C3C 0010                  		move.w	#$10,d6	; set initial shift value
0000147A 6100                       		bsr.s	NemDec_ProcessCompressedData
